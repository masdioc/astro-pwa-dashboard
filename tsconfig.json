{
  "compilerOptions": {
    
     "allowSyntheticDefaultImports": true, 
    "strict": true, 
    "jsx": "preserve",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "types": ["astro/astro-jsx"],
 "paths": {
      "store": ["./src/components/vendor/store"],
          "@dto/*": ["./src/components/dto/*"],
      "@vendor/*": ["./src/components/vendor/*"],
      "@/*": ["./src/*"]
    },
  },
  "include": ["src"]
}

