{
  "nomor": "112",
  "nama": "Al-Ikhlas",
  "ayat": [
    {
      "arabic": "قُلۡ هُوَ ٱللَّهُ أَحَدٌ",
      "latin": "Qul huwa Allahu ahadun",
      "translation": "Katakanlah (Muhammad), \"Dialah Allah, Yang Maha Esa"
    },
    {
      "arabic": "ٱللَّهُ ٱلصَّمَدُ",
      "latin": "Allahu alssamadu",
      "translation": "Allah tempat meminta segala sesuatu"
    },
    {
      "arabic": "لَمۡ يَلِدۡ وَلَمۡ يُولَدۡ",
      "latin": "Lam yalid walam yooladu",
      "translation": "(Allah) tidak beranak dan tidak pula diperanakkan"
    },
    {
      "arabic": "وَلَمۡ يَكُن لَّهُۥ كُفُوًا أَحَدُۢ",
      "latin": "Walam yakun lahu kufuwan ahadun",
      "translation": "Dan tidak ada sesuatu yang setara dengan Dia"
    }
  ]
}